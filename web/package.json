{
  "name": "shifu-web",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev & npm run build & npm run deploy",
    "build": "nodemon --watch main.js --watch to_markdown.js --exec \"cross-env NODE_ENV=production webpack --mode production --config webpack.config.js --progress\"",
    "build-cloud": "cross-env NODE_ENV=production webpack --mode production --config webpack.config.js --progress",
    "deploy": "cross-env NODE_ENV=dev webpack --watch --mode production --config webpack.config.js --progress",
    "dev": "nodemon --watch package.json --watch webpack.config.js --exec \"cross-env NODE_ENV=development npx webpack serve --mode development --progress --hot \"",
    "test": "karma start test/unit/karma.conf.js --single-run"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "babel-loader": "^8.2.5",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "sounds-webpack-plugin": "^0.0.2",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "dependencies": {
    "highlight.js": "^11.6.0",
    "katex": "^0.16.0",
    "marked": "^4.0.18",
    "grogu": "../../grogu"
  }
}
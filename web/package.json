{
  "name": "web",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --watch package.json --watch webpack.config.js --exec \"cross-env NODE_ENV=development npx webpack serve --mode development --progress --hot \"",
    "build": "cross-env NODE_ENV=production webpack --mode production --config webpack.config.js --progress",
    "deploy": "cross-env NODE_ENV=copy webpack --watch --mode production --config webpack.config.js --progress",
    "test": "karma start test/unit/karma.conf.js --single-run"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^10.2.4",
    "webpack": "^5.68.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "highlight.js": "^11.4.0",
    "marked": "^4.0.12"
  }
}
